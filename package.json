{
  "name": "resulti",
  "version": "1.0.0",
  "main": "dist/resulti.cjs.js",
  "module": "dist/resulti.esm.js",
  "description": "A Rustism meant to make error handling much less error prone and more mandatory",
  "files": [
    "/dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write '**/*.js'",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "result",
    "rust",
    "rustism"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mathspy/resulti.git"
  },
  "author": "Mathspy <mathspy257@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "rollup": "^1.6.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
